@article{bhadra_cellular_2018,
	title = {Cellular reagents for diagnostics and synthetic biology},
	volume = {13},
	issn = {1932-6203},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0201681},
	doi = {10.1371/journal.pone.0201681},
	abstract = {We have found that the overproduction of enzymes in bacteria followed by their lyophilization leads to 'cellular reagents' that can be directly used to carry out numerous molecular biology reactions. We demonstrate the use of cellular reagents in a variety of molecular diagnostics, such as TaqMan qPCR with no diminution in sensitivity, and in synthetic biology cornerstones such as the Gibson assembly of DNA fragments, where new plasmids can be constructed solely based on adding cellular reagents. Cellular reagents have significantly reduced complexity and cost of production, storage and implementation, features that should facilitate accessibility and use in resource-poor conditions.},
